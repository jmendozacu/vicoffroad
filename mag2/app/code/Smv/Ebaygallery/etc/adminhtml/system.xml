                <source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
            </field>
            <field id="token" translate="label" type="text" sortOrder="6" showInDefault="6" showInWebsite="6" showInStore="6">
                <label>Ebay Token</label>
                <source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
            </field>
        </group>
   </section>
</system>
</config>